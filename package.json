{
  "name": "shopify-node-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prod": "yarn run clean && yarn run build && cross-env NODE_ENV=production yarn run start",
    "dev": "cross-env NODE_ENV=development yarn run start",
    "start": "nodemon ./bin/www",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.js --progress --profile --colors",
    "clean": "rm -rf ./assets && mkdir ./assets",
    "pretty": "prettier --single-quote --trailing-comma es5 --write {client,bin,config,server}{/*,/**/*}.js",
    "precommit": "yarn run pretty",
    "heroku-prebuild": "yarn install --production=false",
    "postinstall": "NODE_ENV=production webpack --config ./config/webpack.config.js"
  },
  "engines": {
    "node": "8.11.2"
  },
  "browsers": [
    "last 3 chrome versions",
    "last 3 firefox versions",
    "last 2 versions",
    "safari >= 8",
    "ios >= 8",
    "ie >= 11",
    "explorermobile >= 11",
    "android >= 4.4"
  ],
  "dependencies": {
    "@material-ui/core": "^1.0.0",
    "@shopify/polaris": "^2.1.1",
    "@shopify/shopify-express": "^1.0.0-alpha.7",
    "axios": "^0.18.0",
    "chalk": "^1.1.3",
    "chart.js": "^2.7.2",
    "connect-redis": "^3.3.0",
    "cross-env": "^5.1.5",
    "debug": "~2.6.3",
    "dotenv": "^4.0.0",
    "ejs": "^2.6.1",
    "express": "~4.15.2",
    "express-session": "^1.15.3",
    "knex": "^0.13.0",
    "morgan": "~1.8.1",
    "nodemon": "^1.17.4",
    "react": "^16.3.2",
    "react-bootstrap": "^0.32.1",
    "react-chartjs-2": "^2.7.2",
    "react-dom": "^16.3.2",
    "react-object-inspector": "^0.2.1",
    "react-redux": "^5.0.7",
    "react-stringify": "^1.0.0",
    "redis": "^2.7.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "shopify-api-node": "^2.14.0",
    "url": "^0.11.0",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-middleware": "^1.5.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "3.19.0",
    "eslint-plugin-prettier": "^2.6.0",
    "global": "^4.3.2",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.12.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.11.2"
  }
}
